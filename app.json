{
  "expo": {
    "name": "LlamaLang",
    "slug": "llamalang",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/llama_without_background.png",
      "resizeMode": "contain",
      "backgroundColor": "#e5b3bb"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/llama_without_background.png",
        "backgroundColor": "#e5b3bb"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.llamalang.llamalang"
    },
    "owner": "llamalang"
  }
}
